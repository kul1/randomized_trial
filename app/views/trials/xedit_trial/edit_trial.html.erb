<%
  trial = Trial.find_by :id=> $xvars["p"]["trial_id"]
%>
<%= fields_for trial do |f| %>
  <%= f.label :title, "Title" %>
  <%= f.text_field :title %>
  <%= f.label :estart, "Enrollment Start" %>
  <%= f.text_field :estart,"data-role"=>"datebox" %>
  <%= f.label :eend, "Enrollment End" %>
  <%= f.text_field :eend,"data-role"=>"datebox" %>
  <%= f.label :estatus, "Status" , :class=>"ui-input-text" %>
  <%= f.select :status, [['Open',0],['Pending',1],['Schedule',2]],{},"data-native-menu"=>"false" %>
<% end %>


