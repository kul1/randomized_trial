<%
  trial = Trial.find_by :id=> $xvars["select_trial"]["title"]
%>
<%= fields_for trial do |f| %>
  <%= f.label :title, "Title" %>
  <%= f.text_field :title %>
  <%= f.label :title, "Enrollment Start" %>
  <%= f.date_field :start %>
  <%= f.label :title, "Enrollment End" %>
  <%= f.date_field :end %>   <div>
  <%= f.label :status, "Status" , :class=>"ui-input-text" %>
  <%= f.select :status, [['Open',0],['Pending',1],['Schedule',2]],{},"data-native-menu"=>"false" %>
  </div>

<% end %>